using System;

public class LargestNumber
{
    static int findLargestNum(int num1, int num2, int num3)
    {
        if(num1>num2 && num1>num3)
             return num1;
        else if (num2>num3)
            return num2;
        else  
            return num3;
    }
   
    static void Main(string[] args)
    {
        int num1 = 0;
        int num2 = 2;
        int num3 = 1;
        Console.WriteLine(findLargestNum(num1, num2, num3));
        
    }
}