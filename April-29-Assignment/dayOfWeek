using System;

public class DayOfWeek
{
    static  string day(int num)
    {
        switch(num)
        {
            case 1:
                 return "MONDAY";
            case 2:
                 return "TUESDAY";
            case 3:
                 return "WEDNESDAY";
            case 4:
                 return "FRIDAY";
            case 5:
                 return "THURSDAY";
            case 6:
                 return "FRIDAY";
            case 7:
                 return "SATURDAY";
            default :
                 return "INVALID INPUT";
        }
    }
   
    static void Main(string[] args)
    {
        int num = 3;
        Console.WriteLine(day(num));
        
    }
}
